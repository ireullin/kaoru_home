
<div class="container-fluid">

	<div class="page-header">
		<h1><strong>for debug rank</strong></h1>
	</div>

	<table class="table table-hover">
    <thead>
    	<tr class="info">
        	<% 1.upto(@max) do |i| %>
        		<th>No <%= i.to_s%></th>
        	<% end %>
      	</tr>
    </thead>

    <tbody>
    	<% 1.upto(@max) do |i| %>
    		<tr>
    			<% 0.upto(@max-1) do |j| %>
        			<td>
        				<b><%= @arr_af_rank[i][:numbers][j] %></b>
        				<br>
        				<%= @arr_af_rank[i][:count][j] %>
        				<% if j!=0 %>
        					<br>
        					<%= (@arr_af_rank[i][:count][j].to_f / @arr_af_rank[i][:count][0].to_f * 100 ).to_i %>%
        				<% end %>
        			</td>
        		<% end %>
        	</tr>
        <% end %>
    </tbody>
</table>


</div>


<h1><%= params[:type]=='superlottos' ? 'superlottos':'lottery649' %></h1>

<%= paginate @data %>
<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Term</th>
      <th>No 1</th>
      <th>No 2</th>
      <th>No 3</th>
      <th>No 4</th>
      <th>No 5</th>
      <th>No 6</th>
      <th>Special</th>
      <th>Annouced date</th>
      <th>Created date</th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |row| %>
      <tr>
        <td><%= row.term %></td>
        <td><%= row.no1 %></td>
        <td><%= row.no2 %></td>
        <td><%= row.no3 %></td>
        <td><%= row.no4 %></td>
        <td><%= row.no5 %></td>
        <td><%= row.no6 %></td>
        <td><%= row.special %></td>
        <td><%= row.announced_at %></td>
        <td><%= row.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @data %>
